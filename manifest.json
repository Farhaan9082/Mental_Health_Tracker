{
    "manifest_version": 3,
    "name": "Mental Wellbeing",
    "version": "0.1.0",

    "action": {
        "default_icon": "images/favicon.png",
        "default_title": "Get my sentiment analysis score",
        "default_popup": "popup.html"
    },

    "description": "Performs sentiment check on the websites you visit.",
    "icons": {
        "128": "images/icon.png"
    },
    "permissions": [
        "history",
        "tabs",
        "activeTab",
        "scripting"
    ]
}